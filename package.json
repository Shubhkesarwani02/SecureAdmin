{
  "name": "superadmin-framtt",
  "version": "1.0.0",
  "description": "Framtt Superadmin Dashboard - Full Stack Application",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run frontend:dev\" \"npm run backend:dev\"",
    "build": "npm run frontend:build",
    "frontend:dev": "cd frontend && npm run dev",
    "frontend:build": "cd frontend && npm run build",
    "frontend:preview": "cd frontend && npm run preview",
    "backend:dev": "cd backend && npm run dev",
    "backend:start": "cd backend && npm run start",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install",
    "clean": "rm -rf frontend/node_modules backend/node_modules node_modules frontend/dist",
    "lint": "cd frontend && npm run lint",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:migrate:reset": "prisma migrate reset",
    "prisma:db:pull": "prisma db pull",
    "prisma:db:push": "prisma db push",
    "prisma:format": "prisma format"
  },
  "workspaces": [
    "frontend",
    "backend"
  ],
  "devDependencies": {
    "concurrently": "^8.2.2",
    "prisma": "^6.19.0"
  },
  "keywords": [
    "framtt",
    "superadmin",
    "dashboard",
    "react",
    "typescript",
    "vite",
    "express",
    "node"
  ],
  "author": "Framtt Team",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "@supabase/supabase-js": "^2.55.0",
    "dotenv": "^17.2.1",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.10",
    "pdfkit": "^0.17.1"
  }
}
